# Configuration file for LCOV
# Exclude patterns for LCOV (regular expressions)
exclude_pattern = .*\.freezed\.dart$
# Include patterns (regular expressions)
include_pattern = .*\.dart$

# Branch coverage
genhtml_branch_coverage = 1

# Don't show test data
lcov_branch_coverage = 1
lcov_function_coverage = 1

# Exclude generated files
genhtml_exclude_unreachable_branches = 1
genhtml_ignore_empty = 1
